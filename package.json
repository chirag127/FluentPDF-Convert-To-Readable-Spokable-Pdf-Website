{
    "name": "fluentpdf",
    "version": "1.1.0",
    "description": "FluentPDF is a powerful, browser-based Single Page Application (SPA) designed to convert technical documents (PDFs) into engaging audio scripts suitable for Text-to-Speech (TTS) engines. It leverages advanced Large Language Models (LLMs) to intelligently rewrite raw code, tables, diagrams, and technical jargon into natural, descriptive English.",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "dev": "vite",
        "test": "vitest run",
        "test:watch": "vitest",
        "build": "vite build",
        "lint": "biome check .",
        "format": "biome check --write ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chirag127/FluentPDF-Convert-To-Readable-Spokable-Pdf-Website.git"
    },
    "keywords": ["pdf", "llm", "tts", "accessibility", "ai"],
    "author": "Chirag127",
    "license": "MIT",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.5.1",
        "clsx": "^2.1.0",
        "idb": "^8.0.0",
        "jspdf": "^3.0.2",
        "marked": "^12.0.0",
        "pdfjs-dist": "^4.4.168",
        "sortablejs": "^1.15.2",
        "tailwind-merge": "^2.2.1"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.5.3",
        "@types/node": "^20.11.19",
        "@types/sortablejs": "^1.15.8",
        "autoprefixer": "^10.4.17",
        "fake-indexeddb": "^6.2.5",
        "happy-dom": "^13.3.8",
        "postcss": "^8.4.35",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.3.3",
        "vite": "^5.1.4",
        "vite-plugin-singlefile": "^2.0.1",
        "vitest": "^1.3.1"
    }
}
